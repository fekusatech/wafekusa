			scales: {
				xAxes: [{
					scaleLabel: {
						display: true,
						labelString: 'Date'
					},
					gridLines: {
						display: false,
					},
					ticks: {
						autoSkip: false
					},
					stacked: true
				}],
				yAxes: [{
					id: "A",
					type: 'linear',
					position: 'left',
					scaleLabel: {
						display: true,
					},
					ticks: {
						beginAtZero: true,
						stepSize: 2,
						callback: function(value) {
							if (value % 1 === 0) {
								return value;
							}
						}
					},
					gridLines: {
						display: true,
					},
					stacked: true

				}]
			}

		}

		// This will get the first returned node in the jQuery collection.
		var areaChartbw = new Chart(areaChartCanvasbw, {
			type: 'bar',
			data: areaChartDatabw,
			options: areaChartOptionsbw
		})
	}

	function printDiv() {
		$("#printdiv").show();
		window.print();
	}
</script>
<!-- /.content-wrapper --><!-- /.content-wrapper -->

<script>
    $(function() {
        $('.select2tags').select2({
            tags: true,
            theme: 'bootstrap4',
            allowClear: true,
            placeholder: "---",
        });
        
        $(".decimal").inputmask({
            alias: "decimal",
            integerDigits: 9,
            digits: 2,
            digitsOptional: false,
            placeholder: "0",
            allowMinus: false
        });
        
        $(".uang").inputmask({
            alias: "currency",
            prefix: '',
            digits: '0',
            allowMinus: 'false',
            showMaskOnFocus: 'false',
            showMaskOnHover: 'false',
        });
        $('.date').datetimepicker({
            format: 'DD/MM/YYYY',
            autoclose: true,
            todayHighlight: true,
            minuteStepping: 1,               //set the minute stepping
           
            // minDate:'1900/1/1',               //set a minimum date  <---- HERE 
            maxDate: moment(),
            widgetPositioning:{
                                horizontal: 'auto',
                                vertical: 'bottom'
                            }
        });
        
        
        // $('.date').datetimepicker()
        $('.select2').select2({
            placeholder: "---",
            allowClear: true
        });
        $('.select2bs4').select2({
            theme: 'bootstrap4',
            allowClear: true,
            placeholder: '---'
        });
        $('#btnLogout').on('click', function() {
            swal
                .fire({
                    title: "Perhatian",
                    text: "Apakah anda yakin ingin keluar?",
                    showCloseButton: true,
                    showCancelButton: true,
                    confirmButtonColor: "#DD6B55",
                    confirmButtonText: "Keluar",
                    cancelButtonText: "Tidak"
                })
                .then(function(result) {
                    if (result.value) {
                        window.location.href = window.location.origin + "/unit.php";
                    } else {}
                });
        });
    })

    function showToast(params) {
        const Toast = Swal.mixin({
            toast: true,
            position: 'top-end',
            showConfirmButton: false,
            timer: 3000
        });
        Toast.fire({
            icon: 'success',
            title: params
        })
    }

    function showToastError(params) {
        const Toast = Swal.mixin({
            toast: true,
            position: 'top-end',
            showConfirmButton: false,
            timer: 3000
        });
        Toast.fire({
            icon: 'error',
            title: params
        })
    }

    function showToastWarning(params) {
        const Toast = Swal.mixin({
            toast: true,
            position: 'top-end',
            showConfirmButton: false,
            timer: 3000
        });
        Toast.fire({
            icon: 'warning',
            title: params
        })
    }
</script>
